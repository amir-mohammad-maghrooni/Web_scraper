{
    "base_url": "https://quotes.toscrape.com",
    "output_file": "data.csv",
    "output_format": "csv",
    "selectors": {
        "Quotes": "div.quote span.text",
        "Author": "div.quote span small.author",
        "Tags": "div.quote div.tags a"
    },
    "pagination_selector": "li.next a"
}